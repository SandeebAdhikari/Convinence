<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.convinence">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Convinence">
        <activity
            android:name=".cartActivity"
            android:exported="false"
            android:label="Cart"/>
        <activity
            android:name=".CategoriesDiscount"
            android:exported="false"
            android:label="Discount" />
        <activity
            android:name=".CategoriesBags"
            android:exported="false"
            android:label="Bags" />
        <activity
            android:name=".CategoriesAccessories"
            android:exported="false"
            android:label="Accessories" />
        <activity
            android:name=".CategoriesDevices"
            android:exported="false"
            android:label="Devices" />
        <activity
            android:name=".DetailActivity"
            android:exported="false"
            android:label="Details" />
        <activity
            android:name=".ListOfItems"
            android:exported="false"
            android:label="Items" />
        <activity
            android:name=".itemCategories"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>